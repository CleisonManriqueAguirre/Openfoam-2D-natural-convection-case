FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 30
);

regions
(
    boxToFace
    {
        box (2 -1000 0) (3 1e-5 0.1);

        fieldValues
        (
            volScalarFieldValue T 500
        );
    }
);


